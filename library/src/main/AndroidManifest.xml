<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools" package="com.proxyrack">

    <uses-permission android:name="android.permission.INTERNET"/>

    <!--TODO: Check if usesCleartextTraffic=true should be added to client apps' manifests, if building as AAR-->
    <application
            android:usesCleartextTraffic="true"
            tools:ignore="GoogleAppIndexingWarning">

        <service android:name="net.monetizemyapp.android.ProxyService"/>

        <receiver android:name="net.monetizemyapp.android.BootReceiver">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED"/>
            </intent-filter>
        </receiver>

    </application>

</manifest>